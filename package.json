{
  "name": "firebase-ivy-live-feed",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl --proxy-config=proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "analyze": "ng build --prod --source-map=true --stats-json && webpack-bundle-analyzer ./dist/firebase-ivy-live-feed/stats-es2015.json",
    "sourcemap": "source-map-explorer dist/firebase-ivy-live-feed/*.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.2",
    "@angular/cdk": "~9.2.1",
    "@angular/common": "~9.1.2",
