    "@angular/core": "~9.1.2",
    "@angular/fire": "^6.0.0",
    "@angular/forms": "~9.1.2",
    "@angular/material": "^9.2.1",
    "@angular/platform-browser": "~9.1.2",
    "@angular/platform-browser-dynamic": "~9.1.2",
    "@angular/router": "~9.1.2",
    "rxjs": "~6.5.5",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3",
    "firebase": "7.22.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.901.1",
    "@angular-devkit/build-angular": "~0.901.1",
